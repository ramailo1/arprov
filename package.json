    "build": "gradle build",
    "test": "gradle test",
    "clean": "gradle clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramailo1/arprov.git"
  },
  "keywords": [
    "cloudstream",
    "extensions",
    "arabic",
    "streaming",
    "video",
    "movies",
    "series",
    "anime",
    "providers",
    "extractors",
    "android",
    "kotlin",
    "media",
    "entertainment"
  ],
  "author": "ramailo1 <ramailo1@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramailo1/arprov/issues"
  },
  "homepage": "https://github.com/ramailo1/arprov#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "js-yaml": "^4.1.0",
    "chalk": "^5.3.0"
  },
  "files": [
    "build/",
    "docs/",
    "scripts/",
    "*.md",
    "*.json",
    "*.gradle.kts",
    "gradle/",
    "gradlew*"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests",
    "script": "scripts",
    "example": "examples"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "CloudStream Extensions Arabic",
        "icon": "assets/icon.ico"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
